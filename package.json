{
  "name": "vue-tel-input",
  "version": "4.3.0",
  "description": "International Telephone Input with Vue",
  "author": "Steven Dao <iamstevendao@gmail.com>",
  "license": "MIT",
  "homepage": "https://educationlink.github.io/vue-tel-input/",
  "repository": {
    "type": "git",
    "url": "https://github.com/EducationLink/vue-tel-input.git"
  },
  "scripts": {
    "dev": "vue-cli-service serve",
    "demo": "vue-cli-service build",
    "build": "set NODE_ENV=production && npm run build:browser && npm run build:es && npm run build:umd",
    "build:browser": "rollup --config build/rollup.config.browser.js",
    "build:es": "rollup --config build/rollup.config.es.js",
    "build:umd": "rollup --config build/rollup.config.umd.js",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "files": [
    "dist/"
  ],
  "main": "dist/vue-tel-input.umd.js",
  "module": "dist/vue-tel-input.esm.js",
  "unpkg": "dist/vue-tel-input.min.js",
  "dependencies": {
    "awesome-phonenumber": "^2.26.0",
    "libphonenumber-js": "^1.7.38"
  },
  "devDependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-plugin-unit-jest": "^4.2.2",
    "@vue/cli-service": "^4.2.2",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "core-js": "^2.6.11",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^8.1.5",
    "rollup": "^1.31.0",
    "rollup-plugin-analyzer": "^3.2.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-css-only": "^1.0.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-postcss": "^2.0.6",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-vue": "^5.1.6",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "keywords": [
    "vue",
    "telephone",
    "phone",
    "vue-tel-input",
    "phone input",
    "telephone input",
    "input",
    "international phone"
  ]
}
